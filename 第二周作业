    console.log("第三章第一题")
const min=function(n,m){
    let value=n+m;
    return value-(n>m ?n:m);
    
};
console.log(min(20,4));

  </script>
<script>
    console.log("这是作业的第二题");
    const isEven =function(x){
      let remainder =x%2;
      if(remainder===0){
        return true
      } else if(remainder!=0){
      return false
      }
    }
    console.log(isEven());



<script>
console.log("这是第三章的第三题")
console.log("这是第一小题")
const  countBs=function(x){
   let count=0
for(let i=0;i<=x.length;i++){
 
  if(x[i]==="B"){
    count++
  }
  }
  return count;
}
console.log(countBs("BBBBBBBBook"));

console.log("这是第二小题");
const countChar=function(x,y){
  let count=0
  for(let i=0;i<=x.length;i++){
  if(x[i]===y){
    count++
  }
}
return count;
}
console.log(countChar("WWWWhy","W"))

  </script>


<script>
      console.log("这是第四章作业");
      const range = function (x, y) {
        let count = 0;
        for (let i = x; i <= y; i++) {
          console.log( i);
        }
      };
      console.log(range(2, 9));
const sum=function(x,y){
  let count=0
  for (let i=x;i<=y;i++){
    count+=i;
  }
  return count;
};
console.log(sum(3,6))   
</script>

 <script>
      console.log("第四章第二题");

      let myArray =["x","y","z"] 
let arrayvalue=[1,2,3,4,5];
const reverseArray=function(arr){
  let reverseInplace=[];
  for(let i=arr.length-1;i>=0 ;i--){
reverseInplace.push(arr[i]); 
  }
  return reverseInplace
};
console.log(reverseArray(myArray));
console.log(reverseArray(arrayvalue));
</script>


